%% Track Simulations

% Read track information
track = importdata('st_augustin_01_2_2km.csv', ';', 0);
travelledDistance = track(:,1);
elevation = track(:,2);

% Plot Hight Profile
figure(1);clf;hold on;
plot(travelledDistance, elevation);
xlabel('travelled distance');
ylabel('Elevation');


% Simulate
r = simulateTrack(track);


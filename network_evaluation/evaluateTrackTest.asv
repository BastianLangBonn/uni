clear;
addpath('./../trainingData/');
data = load('data.mat');
data = data.data;

%% Create Test & Training Data
permutation = randperm(length(data));
for i=1:183
  testData(i) = data(permutation(i));
end
for i=184:length(data)
  trainingData(i-183) = data(permutation(i));
end


input = [data(1).command' data(1).currentVelocity' data(1).slope'];
input = normalizeInputData(input);
expectedOutput = [data(1).resultingVelocity' data(1).energy'];
expectedOutput = normalizeOutputData(expectedOutput);
weightMatrix = ones(1+3+1+2, 1+2);
fitness = evaluateTrack(weightMatrix, input, expectedOutput);

for i=1:length(data)
  maxValue(i) = max(data(i).energy);
end
maxValue = maxValue(i);

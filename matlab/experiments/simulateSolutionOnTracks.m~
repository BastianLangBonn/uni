function [] = simulateSolutionOnTracks(ind, p)


for i=1:length(p.tracks)        
    p.track = p.tracks(i);
    [fitness{i}, time{i}, location{i}, speed{i}...
      , energy{i}, command{i}] = evaluateOnTrack(ind, p);
    figure(i); clf;
    subplot(2,2,1);
    plot(command{i});
    title('Command over time');
    xlabel('Command');
    ylabel('Time in s');
    gri
    subplot(2,2,2);
    plot(speed{i});
    subplot(2,2,3);
    semilogy(energy{i});

end